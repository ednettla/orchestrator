{
  "name": "orchestrator",
  "version": "0.1.8",
  "description": "Claude Code orchestrator for building full-stack web applications with multi-agent pipelines",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "orchestrate": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/cli/index.js",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "release": "npm run version:patch && npm run build && git add -A && git commit -m \"chore: bump version to $(node -p \"require('./package.json').version\")\" && git push"
  },
  "keywords": [
    "claude",
    "ai",
    "orchestrator",
    "code-generation",
    "multi-agent"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/claude-code": "^0.2.57",
    "better-sqlite3": "^11.7.0",
    "chalk": "^5.4.1",
    "commander": "^13.0.0",
    "ink": "^6.6.0",
    "ink-spinner": "^5.0.0",
    "inquirer": "^12.3.2",
    "nanoid": "^5.0.9",
    "ora": "^8.1.1",
    "react": "^19.2.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.10.5",
    "@types/react": "^19.2.7",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
